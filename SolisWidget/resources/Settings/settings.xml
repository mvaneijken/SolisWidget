<resources>
	<properties>
        <property id="PROP_STARTPAGE" type="number">1</property>
        <property id="PROP_USERNAME" type="string"></property>
        <property id="PROP_PASSWORD" type="string"></property>
        <property id="PROP_UID" type="string"></property>
        <property id="PROP_PLANTID" type="string"></property>
	</properties>
	<settings>
        <setting propertyKey="@Properties.PROP_STARTPAGE" title="@Strings.title_startpage">
            <settingConfig type="list">
            	<listEntry value="1">@Strings.opt_page_1</listEntry>
            	<listEntry value="2">@Strings.opt_page_2</listEntry>
            	<listEntry value="6">@Strings.opt_page_6</listEntry>
            </settingConfig>
        </setting>
        <setting propertyKey="@Properties.PROP_USERNAME" title="@Strings.title_username">
        	<settingConfig type="AlphaNumeric" required="false"></settingConfig>
        </setting>
        <setting propertyKey="@Properties.PROP_PASSWORD" title="@Strings.title_password">
        	<settingConfig type="Password" required="false"></settingConfig>
        </setting>
		<setting propertyKey="@Properties.PROP_UID" title="@Strings.title_uid">
        	<settingConfig type="AlphaNumeric" required="false" readonly="true"></settingConfig>
        </setting>
		<setting propertyKey="@Properties.PROP_PLANTID" title="@Strings.title_plantid">
        	<settingConfig type="AlphaNumeric" required="false" readonly="true"></settingConfig>
        </setting>
	</settings>
</resources>
