<resources>
	<properties>
        <property id="PROP_STARTPAGE" type="number">1</property>
        <property id="PROP_USERNAME" type="string"></property>
        <property id="PROP_PASSWORD" type="string"></property>
        <property id="PROP_UID" type="string"></property>
        <property id="PROP_PLANTID" type="string"></property>
	</properties>
	<settings>
        <setting propertyKey="@Properties.PROP_STARTPAGE" title="@Strings.ttlstrtpg">
            <settingConfig type="list">
            	<listEntry value="1">@Strings.optpg1</listEntry>
            	<listEntry value="2">@Strings.optpg2</listEntry>
                <listEntry value="3">@Strings.optpg3</listEntry>
                <listEntry value="4">@Strings.optpg4</listEntry>
                <listEntry value="5">@Strings.optpg5</listEntry>
            	<listEntry value="6">@Strings.optpg6</listEntry>
            </settingConfig>
        </setting>
        <setting propertyKey="@Properties.PROP_USERNAME" title="@Strings.titleusr">
        	<settingConfig type="AlphaNumeric" required="false"></settingConfig>
        </setting>
        <setting propertyKey="@Properties.PROP_PASSWORD" title="@Strings.titlepswrd">
        	<settingConfig type="Password" required="false"></settingConfig>
        </setting>
		<setting propertyKey="@Properties.PROP_UID" title="@Strings.titleuid">
        	<settingConfig type="AlphaNumeric" required="false" readonly="true"></settingConfig>
        </setting>
		<setting propertyKey="@Properties.PROP_PLANTID" title="@Strings.titleplantid">
        	<settingConfig type="AlphaNumeric" required="false" readonly="true"></settingConfig>
        </setting>
	</settings>
</resources>
