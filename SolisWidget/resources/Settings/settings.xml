<resources>
    <properties>
        <property id="PSP" type="number">1</property>
        <property id="PUN" type="string"></property>
        <property id="PPS" type="string"></property>
        <property id="PUI" type="number">-1</property>
        <property id="PPI" type="number">-1</property>
    </properties>
    <settings>
        <setting propertyKey="@Properties.PSP" title="@Strings.SP">
            <settingConfig type="list">
                <listEntry value="1">@Strings.O1</listEntry>
                <listEntry value="2">@Strings.O2</listEntry>
                <listEntry value="3">@Strings.O3</listEntry>
                <listEntry value="4">@Strings.O4</listEntry>
                <listEntry value="5">@Strings.O5</listEntry>
                <listEntry value="6">@Strings.O6</listEntry>
            </settingConfig>
        </setting>
        <setting propertyKey="@Properties.PUN" title="@Strings.C1">
            <settingConfig type="AlphaNumeric" required="false"></settingConfig>
        </setting>
        <setting propertyKey="@Properties.PPS" title="@Strings.C2">
            <settingConfig type="Password" required="false"></settingConfig>
        </setting>
        <setting propertyKey="@Properties.PUI" title="@Strings.UI">
            <settingConfig type="Numeric" required="false" readonly="true"></settingConfig>
        </setting>
        <setting propertyKey="@Properties.PPI" title="@Strings.PI">
            <settingConfig type="Numeric" required="false" readonly="true"></settingConfig>
        </setting>
    </settings>
</resources>
